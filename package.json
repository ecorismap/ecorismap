  "private": true,
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "./jestSetupFile.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?@react-native|react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*|react-redux)"
    ],
    "testPathIgnorePatterns": [
      "src/__tests__/resources/.*",
      "src/lib/__tests__/firebase/.*",
      "maplibre-native/.*"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/__tests__/**",
      "!src/**/index.ts",
      "!src/constants/**",
      "!src/types/**",
      "!src/i18n/**",
      "!src/licenses.json"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html",
      "json"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 20,
        "functions": 20,
        "lines": 20,
        "statements": 20
      },
      "src/hooks/**/*.ts": {
        "branches": 30,
        "functions": 30,
        "lines": 30,
        "statements": 30
      },
      "src/utils/**/*.ts": {
        "branches": 40,
        "functions": 40,
        "lines": 40,
        "statements": 40
      },
      "src/modules/**/*.ts": {
        "branches": 60,
        "functions": 60,
        "lines": 60,
        "statements": 60
      }
    }
  },
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "android:device": "emulator @Pixel_7_API_34 -no-snapshot-load & expo run:android",
    "ios": "expo run:ios",
    "web": "GENERATE_SOURCEMAP=false PLATFORM=web expo start --web",
    "build:web": "expo export:web",
    "test": "jest --detectOpenHandles --runInBand --forceExit",
    "test:coverage": "jest --coverage --detectOpenHandles --runInBand --forceExit",
    "test:coverage:watch": "jest --coverage --watchAll --detectOpenHandles --runInBand --forceExit",
    "testemu": "firebase emulators:exec 'yarn jest'",
    "lint": "eslint --fix --ext .tsx --ext .ts src/",
    "audit": "yarn npm audit",
    "emu": "firebase emulators:start --import=./emulatorExport --export-on-exit",
    "postinstall": "patch-package --verbose",
    "firebase:dev": "node scripts/switch-firebase-env.js development",
    "firebase:prod": "node scripts/switch-firebase-env.js production",
    "licenses": "npx npm-license-crawler --dependencies --production --onlyDirectDependencies --omitVersion --json ./src/licenses.json"
  },
